namespace upr
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int[] a = new int[10];
            UserArray.inputArr(a);
            UserArray.searchArr(a);
            UserArray.outputArr(a);
            Array.Reverse(a);
            Array.Resize(ref a, 7);
            UserArray.outputArr(a);
            double[,] b = new double[7, 10];
            MultiArray.inputArr(b);
            MultiArray.searchArr(b);
            MultiArray.printArr(b);
        }
    }
}
